{
  "scenario_id": "example-scenario",
  "spec_version": "v1",
  "stages": [
    {
      "stage_id": "main",
      "entry_packets": [
        {
          "packet_id": "packet-hello",
          "schema_id": "schema-hello",
          "content_type": "application/json",
          "visibility_labels": [
            "public"
          ],
          "policy_tags": [],
          "expiry": null,
          "payload": {
            "kind": "json",
            "value": {
              "message": "hello",
              "purpose": "scenario entry packet"
            }
          }
        }
      ],
      "gates": [
        {
          "gate_id": "env_gate",
          "requirement": {
            "Predicate": "env_is_prod"
          }
        },
        {
          "gate_id": "time_gate",
          "requirement": {
            "Predicate": "after_freeze"
          }
        }
      ],
      "advance_to": {
        "kind": "terminal"
      },
      "timeout": null,
      "on_timeout": "fail"
    }

      "query": {
        "provider_id": "env",
        "predicate": "get",
        "params": {
          "key": "DEPLOY_ENV"
        }
      },
      "comparator": "equals",
      "expected": "production",
      "policy_tags": []
    },
    {
      "predicate": "after_freeze",
      "query": {
        "provider_id": "time",
        "predicate": "after",
        "params": {
          "timestamp": 1710000000000
        }
      },
      "comparator": "equals",
      "expected": true,
      "policy_tags": []
    }
  ],
